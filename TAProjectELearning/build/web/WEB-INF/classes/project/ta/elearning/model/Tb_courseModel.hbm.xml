<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="project.ta.elearning.model.Tb_courseModel" table="tb_course">
    <id column="id" name="id">
      <generator class="native"/>
    </id>
    <property length="10" name="category" type="integer"/>
    <property length="254" name="fullname" type="string"/>
    <property length="255" name="shortname" type="string"/>
    <property length="100" name="idnumber" type="string"/>
    <property length="21" name="format" type="string"/>
    <property length="100" name="startdate" type="string"/>
    <property length="100" name="timecreated" type="string"/>
    <property length="100" name="timemodified" type="string"/>
  </class>
</hibernate-mapping>
