<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-configuration PUBLIC "-//Hibernate/Hibernate Configuration DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-configuration-3.0.dtd">
<hibernate-configuration>
  <session-factory>
    <property name="hibernate.dialect">org.hibernate.dialect.MySQLDialect</property>
    <property name="hibernate.connection.driver_class">com.mysql.jdbc.Driver</property>
    <property name="hibernate.connection.url">jdbc:mysql://localhost:3306/e_learning?zeroDateTimeBehavior=convertToNull</property>
    <property name="hibernate.connection.username">root</property>
    <mapping resource="project/ta/elearning/model/UserModel.hbm.xml"/>
    <mapping resource="project/ta/elearning/model/Tb_userModel.hbm.xml"/>
    <mapping resource="project/ta/elearning/model/Tb_courseModel.hbm.xml"/>
    <mapping resource="project/ta/elearning/model/Tb_roleModel.hbm.xml"/>
    <mapping resource="project/ta/elearning/model/Tb_assignmentModel.hbm.xml"/>
    <mapping resource="project/ta/elearning/model/Tb_lessonModel.hbm.xml"/>
    <mapping resource="project/ta/elearning/model/Tb_quizModel.hbm.xml"/>
    <mapping resource="project/ta/elearning/model/Tb_freeQuizModel.hbm.xml"/>
    <mapping resource="project/ta/elearning/model/Tb_levelModel.hbm.xml"/>
    <mapping resource="project/ta/elearning/model/Tb_weklyQuizModel.hbm.xml"/>
    <mapping resource="project/ta/elearning/model/Tb_majorsModel.hbm.xml"/>
    <mapping resource="project/ta/elearning/model/Tb_nilaiModel.hbm.xml"/>
    <mapping resource="project/ta/elearning/model/Tb_materiSyaratModel.hbm.xml"/>
    <mapping resource="project/ta/elearning/model/Tb_fileModel.hbm.xml"/>
  </session-factory>
</hibernate-configuration>
