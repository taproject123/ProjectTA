<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="project.ta.elearning.model.Tb_nilaiModel" table="tb_nilai">
        <id name="id" column="id">
            <generator class="native"/>
        </id>
        <property name="userid" column="userid" type="integer" />
        <property name="nilai_uts" column="nilai_uts" type="double" />
        <property name="nilai_uas" column="nilai_uas" type="double" />
    </class>
</hibernate-mapping>
