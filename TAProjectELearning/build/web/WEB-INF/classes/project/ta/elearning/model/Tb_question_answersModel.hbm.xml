<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="project.ta.elearning.model.Tb_question_answersModel" table="tb_question_answers">
    <id column="id" name="id">
      <generator class="native"/>
    </id>
    <property column="questionid" length="10" name="questionid" type="integer"/>
    <property column="answer" length="255" name="answer" type="string"/>
  </class>
</hibernate-mapping>
