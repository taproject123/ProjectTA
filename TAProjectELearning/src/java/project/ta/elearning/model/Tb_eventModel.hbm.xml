<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="project.ta.elearning.model.Tb_eventModel" table="tb_events">
    <id column="id" name="id">
      <generator class="native"/>
    </id>
    <property column="userid" length="10" name="userid" type="integer"/>
    <property column="courseid" length="10" name="courseid" type="integer"/>
    <property column="name" length="255" name="name" type="string"/>
    <property column="description" length="255" name="description" type="string"/>
    <property column="timestart" length="50" name="timestart" type="string"/>
    <property column="timeduration" length="50" name="timeduration" type="string"/>
    <property column="timemodified" length="50" name="timemodified" type="string"/>
    <property column="timecreated" length="50" name="timecreated" type="string"/>
  </class>
</hibernate-mapping>
