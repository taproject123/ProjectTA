<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="project.ta.elearning.model.Tb_choice_optionsModel" table="tb_choice_options">
    <id column="id" name="id">
      <generator class="native"/>
    </id>
    <property column="userid" length="10" name="userid" type="integer"/>
    <property column="text" length="255" name="text" type="string"/>
    <property column="maxanswer" length="10" name="maxanswer" type="integer"/>
    <property column="timemodified" length="50" name="timemodified" type="string"/>
    <property column="timecreated" length="50" name="timecreated" type="string"/>
  </class>
</hibernate-mapping>
