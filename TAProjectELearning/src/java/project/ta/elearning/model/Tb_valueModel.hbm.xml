<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="project.ta.elearning.model.Tb_valueModel" table="tb_value">
    <id column="id" name="id">
      <generator class="native"/>
    </id>
    <property column="userid" name="userid" type="integer"/>
    <property column="mid_value" name="mid_value" type="double"/>
    <property column="final_value" name="final_value" type="double"/>
  </class>
</hibernate-mapping>
