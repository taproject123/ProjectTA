<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="project.ta.elearning.model.Tb_freeQuizModel" table="tb_free_quiz">
        <id name="id" column="id">
            <generator class="native"/>
        </id>
        <property name="free_quiz" column="free_quiz" type="string" length="255"/>
        <property name="answer" column="answer" type="string" length="255"/> 
        <property name="timeopen" column="timeopen" type="string" length="255"/>
        <property name="timeclose" column="timeclose" type="string" length="255"/>
        <property name="timelimit" column="timelimit" type="string" length="255"/>
        <property name="timecreated" column="timecreated" type="string" length="255"/>
        <property name="timemodified" column="timemodified" type="string" length="255"/>
        <property name="id_course" column="id_course" type="integer" length="20"/>
        <property name="id_materi" column="id_materi" type="integer" length="20"/>
        <property name="id_level" column="id_level" type="integer" length="20"/>
    </class>
</hibernate-mapping>
