<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="project.ta.elearning.model.Tb_fileModel" table="tb_file">
        <id column="id" name="id">
            <generator class="native"/>
        </id>
        <property name="file" column="file" type="string" length="255"/>
        <property name="category" column="category" type="integer" length="10"/>
        <property name="userid" column="userid" type="integer" length="10"/>
    </class>
        
  

</hibernate-mapping>
