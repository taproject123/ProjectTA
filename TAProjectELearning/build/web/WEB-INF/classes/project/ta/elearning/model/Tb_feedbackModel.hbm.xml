<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
   <class name="project.ta.elearning.model.Tb_feedbackModel" table="tb_feedback">
        <id column="id" name="id">
            <generator class="native"/>
        </id>
        <property name="courseid" column="courseid" type="integer" length="10"/>
        <property name="name" column="name" type="string" length="255"/>
        <property name="timeopen" column="timeopen" type="string" length="50"/>
        <property name="timeclose" column="timeclose" type="string" length="50"/>
    </class>
  

</hibernate-mapping>
